<li data-current="1" class="slider__item">
  <a
    target="_blank"
    class="slider__popup thumbnail"
    data-title="[[+title]]"
    data-lightbox="carusel"
    href="[[+image:phpthumbof=`h=800`]]"
  >
    <div class="slider__image">
      <picture>
        <source
          srcset="[[+image:phpthumbof=`h=385&f=webp`]]"
          type="image/webp"
        />
        <img src="[[+image:phpthumbof=`h=385`]]" alt="[[+title]]" />
      </picture>
    </div>
  </a>
</li>
